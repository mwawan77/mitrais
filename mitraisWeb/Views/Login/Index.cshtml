
@{
    ViewBag.Title = "Login";
    ViewBag.CssBody = "login";
}

<div class="row">
    <div class="col-md-2">
    </div>
    <div class="col-md-8">
        <h2>@ViewBag.Title</h2>
        <form id="login_form">
            <div class="form-group">
                <input type="email" class="form-control validate[required,custom[email]]" id="login_email" placeholder="Email">
            </div>
            <div class="form-group">
                <input type="password" class="form-control validate[required]" id="login_password" placeholder="Password">
            </div>
            <button type="button" class="btn btn-default" id="btnLogin">Login</button>
        </form>
    </div>
    <div class="col-md-2">
    </div>
</div>




